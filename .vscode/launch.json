{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "CMake Debug (MinGW64/Ninja)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${command:cmake.launchTargetPath}",
      "cwd": "${workspaceFolder}",
      "MIMode": "gdb",
      "miDebuggerPath": "C:/msys64/mingw64/bin/gdb.exe",
      "externalConsole": true,
      "environment": [
        { "name": "PATH", "value": "C:\\msys64\\mingw64\\bin;${env:PATH}" }
      ],
      "logging": { "engineLogging": false }
    }
  ]
}
